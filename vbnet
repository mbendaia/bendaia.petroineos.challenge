var ctd = new List<string>();
                for (int c = 0, loopTo = DT.Columns.Count - 1; c <= loopTo; c++)
                {
                    string cn = DT.Columns[c].ColumnName;
                    if (cn.Contains("reference"))
                    {
                        ctd.Add(cn);
                    }
                }

                foreach (string cn in ctd)
                    DT.Columns.Remove(cn);

                return DT;
